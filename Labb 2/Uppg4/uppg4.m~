t = [0:0.05:2];

start = [0 5]
slut = [2 5]
y = generalquadraticbezier(t, start, slut, 0);
z = generalcubicbezier(t, start, slut, 0, 1.5);

%q = halfoval(s, 1, 1, 1, 2)

hold on

plot(t,y)
plot(t,z)

centerX = -0.5; centerY = 2.5; radius = 0.5;
rectangle('Position',[centerX - radius, centerY - radius, radius*3/2, radius*2],...
    'Curvature',[1,1],...
    'FaceColor','');

axis equal
hold off